<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <section name="specFlow" type="TechTalk.SpecFlow.Configuration.ConfigurationSectionHandler, TechTalk.SpecFlow" />
  </configSections>
  <log4net>
    <appender name="RollingFile" type="log4net.Appender.RollingFileAppender">
      <file value="./Log/pegasus.Automation.log" />
      <appendToFile value="true" />
      <rollingStyle value="Composite" />
      <datePattern value="yyyyMMdd" />
      <maxSizeRollBackups value="-1" />
      <maximumFileSize value="3MB" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value=" T: %d |L: %p |TH: %t |L: %c{2} |MSG: %m %n" />
      </layout>
      <filter type="log4net.Filter.LevelMatchFilter">
        <LevelToMatch value="error" />
      </filter>
      <filter type="log4net.Filter.LevelMatchFilter">
        <LevelToMatch value="info" />
      </filter>
      <filter type="log4net.Filter.LevelMatchFilter">
        <LevelToMatch value="debug" />
      </filter>
      <filter type="log4net.Filter.DenyAllFilter" />
    </appender>
    <root>
      <level value="info" />
      <appender-ref ref="RollingFile" />
    </root>
    <logger name="Pegasus.Automation">
      <level value="All" />
      <appender-ref ref="RollingFile" />
    </logger>
  </log4net>
  <specFlow>
    <unitTestProvider name="MsTest" />
    <!-- For additional details on SpecFlow configuration options see http://go.specflow.org/doc-config -->
  </specFlow>
  <appSettings>
    <!--Remote Configuration-->
    <add key="isRemote" value="true" />
    <add key="remoteHubUrl" value="http://jenkins.imfinity.net:4444/wd/hub" />

    <!--Internet Explorer, FireFox, GoogleChrome and Safari-->
    <add key="Browser" value="FireFox" />
    <!--Possible values - CGIE, VCD, PROD and PPE-->
    <!-- Please change the values for Execution Environment /-->
    <add key="TestEnvironment" value="PPE"/>

    <!-- CGIE Url /-->
    <add key="CourseSpaceURLRootCGIE" value="http://pegasus5.qa.pegasus.pearsoncmg.com/pegasus/" />
    <add key="WorkSpaceURLRootCGIE" value="http://cg.ws.pegasus5.qa.pegasus.pearsoncmg.com/pegasus/" />

    <!-- PPE URL /-->
    <!--Modified PPE value to include complete login url so that it works even when there are issues with SMS -->
    <add key="CourseSpaceURLRootPPE" value="http://mylabs.px.ppe.pearsoncmg.com/Pegasus/frmLogin.aspx?s=3" />
    <add key="WorkSpaceURLRootPPE" value="http://hed.ws.mylabs.px.ppe.pearsoncmg.com/pegasus/" />

    <!-- PROD URL /-->
    <add key="CourseSpaceURLRootPROD" value="http://mylabs.px.pearsoned.com/Pegasus/" />
    <add key="WorkSpaceURLRootPROD" value="http://hed.ws.mylabs.px.pearsoned.com/Pegasus/" />

    <!-- VCD URL /-->
    <add key="CourseSpaceURLRootVCD" value="http://pegasqausvcd.ecollege-labs.com/pegasus/" />
    <add key="WorkSpaceURLRootVCD" value="http://cgwspegasusqavcd.ecollege-labs.com/pegasus/" />

    <!--Do Not make changes below this-->
    <add key="WaitUntillElementSeconds" value="420" />
    <add key="AssignedToCopyInterval" value="120" />
    <add key="WebDriverTimeOutInSeconds" value="420" />
    <add key="ElementFindTimeOutInSeconds" value="420" />
    <add key="Retry_Count" value="3" />
    <add key="InstanceName" value="HED-HSS"/>
    <add key="TakeScreenShotDuringEntryExit" value="false" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />

    <!-- SMS Admin Url, Access Code and Module Id -->
   
    <!-- SMS PPE /-->
    <add key="SMSAdminURLRootPPE" value="http://registerppe.pearsoncmg.com/reg/" />
    <add key="SMSInstructorAccessCodePPE" value="PEGPEI-PASHM-OSMIC-VARNA-LEWIS-SITES"/>
    <add key="SMSStudentAccessCodePPE" value="PEGPER-SETUP-OSMIC-VARNA-INGOT-HONES"/>
    <add key="SMSModuleIDPPE" value="40153"/>

    <!-- SMS PROD /-->
    <add key="SMSAdminURLRootPROD" value="http://register.cert.pearsoncmg.com/reg/" />
    <add key="SMSInstructorAccessCodePROD" value="PGCIEN-CAPT-SOME-GALA-THOU-FROM"/>
    <add key="SMSStudentAccessCodePROD" value="PGCISN-MALL-SOME-GALA-DASH-KAKI"/>
    <add key="SMSModuleIDPROD" value="2731"/>

  </appSettings>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
      </providers>
    </roleManager>
  </system.web>
</configuration>